# run service as
#POSTGREST_USER=postgrest

# log file
#POSTGREST_LOG=/var/log/postgrest/postgrest.log

# database host
#POSTGREST_DBHOST=localhost

# database host
#POSTGREST_DBPORT=5432

# database to use
#POSTGREST_DBNAME=app

# database user
